// 检测进入驾驶
while (true) {
    // Home键
    await adb.keyevent('3')
    // 等待
    await delay(1000)
    // 点击汽车控制
    await adb.tap(60, 214)
    await delay(1000)
    const jg = await byText('车身', async (xy, node) => {
        await adb.tap(xy)
    })
    if (jg) break
}

