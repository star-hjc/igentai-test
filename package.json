{
  "name": "igentai-test",
  "version": "0.0.0",
  "main": "electron/main/index.js",
  "license": "MIT",
  "scripts": {
    "appview": "vite --port 5555",
    "app": "nodemon --exec electron . 5555 --watch ./electron --ext .js",
    "serve": "concurrently -k \"npm run appview\" \"npm run app\"",
    "preview": "vite preview",
    "build": "vite build  && electron-builder"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.6.0",
    "@codemirror/lang-javascript": "^6.1.7",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.4.0",
    "codemirror": "^6.0.1",
    "element-plus": "^2.3.4",
    "jsonpath": "^1.1.1",
    "pinia": "^2.0.35",
    "serialport": "^11.0.0",
    "vue": "^3.2.47",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.1.6",
    "xml2js": "^0.5.0",
    "xpath": "^0.0.32"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "concurrently": "^8.0.1",
    "electron": "^24.1.3",
    "electron-builder": "^23.6.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "nodemon": "^2.0.22",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.2"
  },
  "build": {
    "appId": "com.star.igentai-test",
    "productName": "igentai-test",
    "copyright": "Copyright Â© 2023 <star>",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "extraResources": {
      "from": "./electron/assets",
      "to": "assets"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "app"
    }
  }
}
